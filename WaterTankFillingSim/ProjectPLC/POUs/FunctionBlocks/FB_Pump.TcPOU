      </Implementation>
    </Method>
    <Action Name="Stop" Id="{04c11640-edd6-4d96-8d47-bee47abe1576}">
      <Implementation>
        <ST><![CDATA[//SET COMMANDS TO STOP
bStartCmd	:= FALSE;
bStopCmd	:= TRUE;]]></ST>
      </Implementation>
    </Action>
