  <POU Name="FB_Pump" Id="{b98365dc-24f0-47c3-9e64-858b2355d9b2}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_Pump
VAR_INPUT
	bStartCmd	: BOOL;
	bStopCmd	: BOOL;
END_VAR
VAR_OUTPUT
END_VAR
VAR
	_bIsOn		: BOOL;
	_rFlowRate	: REAL;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Property Name="bError" Id="{c756b756-88ce-408d-aa93-ded232701c24}">
      <Declaration><![CDATA[PROPERTY bError : BOOL]]></Declaration>
      <Get Name="Get" Id="{600f7e5c-89b6-4434-b45a-59ac25a65c11}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[//ERROR STATE IF 'bStartCmd' IS ON AND PUMP IS NOT ON
bError	:= bStartCmd AND NOT bIsOn;]]></ST>
        </Implementation>
      </Get>
      <Set Name="Set" Id="{c58e52ff-cf32-4305-bb4a-b01c86457804}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[]]></ST>
        </Implementation>
      </Set>
    </Property>
    <Property Name="bIsOn" Id="{ea638808-ea05-4482-b1c7-c3f710b620fe}">
      <Declaration><![CDATA[PROPERTY bIsOn : BOOL]]></Declaration>
      <Get Name="Get" Id="{9c3d3d13-6a5e-4a2e-881b-d839e8aec501}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[bIsOn	:= _bIsOn;]]></ST>
        </Implementation>
      </Get>
      <Set Name="Set" Id="{b158cd58-19c9-4679-84e8-8a82477d3962}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[_bIsOn	:= bIsOn;]]></ST>
        </Implementation>
      </Set>
    </Property>
    <Method Name="Execute" Id="{1d4f8a3a-cfbb-42c7-876e-a93d9f256b72}">
      <Declaration><![CDATA[METHOD Execute : BOOL
VAR_INPUT
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[//CHECK ON STATE
IF bStartCmd THEN
	bIsOn		:= TRUE;
ELSIF bStopCmd THEN
	bIsOn		:= FALSE;
END_IF]]></ST>
      </Implementation>
    </Method>
    <Method Name="FB_init" Id="{6e55d4e2-b785-4b6d-8e8f-440e6e5bae7a}">
      <Declaration><![CDATA[METHOD FB_init : BOOL
VAR_INPUT
	bInitRetains : BOOL; // if TRUE, the retain variables are initialized (warm start / cold start)
	bInCopyCode : BOOL;  // if TRUE, the instance afterwards gets moved into the copy code (online change)
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[//FLOW RATE NITIALIZATION
rFlowRate	:= 50.0;		]]></ST>
      </Implementation>
    </Method>
    <Property Name="rFlowRate" Id="{2c9952d7-7cf3-4e54-9da4-22409be8e6a2}">
